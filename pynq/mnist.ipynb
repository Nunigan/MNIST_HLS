    "def mnist_sw(im):\n",
    "    result = []\n",
    "    for i in range(10000):\n",
    "        data = im[i]\n",
    "        for j in range(3):\n",
    "            data = (data@weight[j]+bias[j])//scales[j]\n",
    "            if j != 2:\n",
